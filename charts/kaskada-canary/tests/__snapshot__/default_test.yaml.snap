manifest should match snapshot:
  1: |
    apiVersion: apps/v1
    kind: Deployment
    metadata:
      labels:
        app.kubernetes.io/instance: my-release
        app.kubernetes.io/managed-by: Helm
        app.kubernetes.io/name: kaskada-canary
        helm.sh/chart: kaskada-canary-0.0.1
      name: my-release-kaskada-canary
      namespace: my-namespace
    spec:
      replicas: 1
      selector:
        matchLabels:
          app.kubernetes.io/instance: my-release
          app.kubernetes.io/name: kaskada-canary
      strategy:
        type: Recreate
      template:
        metadata:
          labels:
            app.kubernetes.io/instance: my-release
            app.kubernetes.io/name: kaskada-canary
        spec:
          containers:
            - command:
                - /bin/sparrow-main
                - serve
              env:
                - name: SPARROW_LOG_FILTER
                  value: egg::=warn,sparrow_=info,info
                - name: SPARROW_LOG_JSON
                  value: "true"
              image: ghcr.io/kaskada-ai/kaskada/engine:latest
              imagePullPolicy: IfNotPresent
              name: engine
              ports:
                - containerPort: 50052
                  name: grpc
                  protocol: TCP
              volumeMounts:
                - mountPath: /data
                  name: kaskada-canary-data
                - mountPath: /tmp
                  name: kaskada-canary-tmp
            - command:
                - /bin/wren
              env:
                - name: DB_DIALECT
                  value: sqlite
                - name: DB_PATH
                  value: /db/kaskada.db
                - name: DB_IN_MEMORY
                  value: "false"
                - name: OBJECT_STORE_PATH
                  value: /data/
                - name: OBJECT_STORE_TYPE
                  value: local
                - name: DEBUG
                  value: "false"
                - name: LOG_FORMAT_JSON
                  value: "true"
              image: ghcr.io/kaskada-ai/kaskada/engine:latest
              imagePullPolicy: IfNotPresent
              name: manager
              ports:
                - containerPort: 50051
                  name: grpc
                  protocol: TCP
                - containerPort: 3365
                  name: rest
                  protocol: TCP
              volumeMounts:
                - mountPath: /data
                  name: kaskada-canary-data
                - mountPath: /db
                  name: kaskada-canary-db
          volumes:
            - name: kaskada-canary-data
              persistentVolumeClaim:
                claimName: kaskada-canary-data
            - name: kaskada-canary-db
              persistentVolumeClaim:
                claimName: kaskada-canary-db
            - ephemeral:
                volumeClaimTemplate:
                  metadata:
                    labels:
                      type: kaskada-canary-tmp
                  spec:
                    accessModes:
                      - ReadWriteOnce
                    resources:
                      requests:
                        storage: 10Gi
                    storageClassName: null
              name: kaskada-canary-tmp
  2: |
    apiVersion: v1
    kind: PersistentVolumeClaim
    metadata:
      name: kaskada-canary-db
      namespace: my-namespace
    spec:
      accessModes:
        - ReadWriteOnce
      resources:
        requests:
          storage: 1Gi
      storageClassName: null
      volumeMode: Filesystem
  3: |
    apiVersion: v1
    kind: PersistentVolumeClaim
    metadata:
      name: kaskada-canary-data
      namespace: my-namespace
    spec:
      accessModes:
        - ReadWriteOnce
      resources:
        requests:
          storage: 100Gi
      storageClassName: null
      volumeMode: Filesystem
  4: |
    apiVersion: v1
    kind: Service
    metadata:
      labels:
        app.kubernetes.io/instance: my-release
        app.kubernetes.io/managed-by: Helm
        app.kubernetes.io/name: kaskada-canary
        helm.sh/chart: kaskada-canary-0.0.1
      name: my-release-kaskada-canary
      namespace: my-namespace
    spec:
      ports:
        - name: grpc
          port: 50051
          protocol: TCP
          targetPort: 50051
        - name: rest
          port: 3365
          protocol: TCP
          targetPort: 3365
      selector:
        app.kubernetes.io/instance: my-release
        app.kubernetes.io/name: kaskada-canary
      type: ClusterIP
