manifest should match snapshot:
  1: |
    apiVersion: apps/v1
    kind: Deployment
    metadata:
      labels:
        app.kubernetes.io/instance: my-release
        app.kubernetes.io/managed-by: Helm
        app.kubernetes.io/name: kaskada-canary
        helm.sh/chart: kaskada-canary-0.0.1
      name: my-release-kaskada-canary
      namespace: s3-with-key
    spec:
      replicas: 1
      selector:
        matchLabels:
          app.kubernetes.io/instance: my-release
          app.kubernetes.io/name: kaskada-canary
      strategy:
        type: Recreate
      template:
        metadata:
          labels:
            app.kubernetes.io/instance: my-release
            app.kubernetes.io/name: kaskada-canary
        spec:
          containers:
            - command:
                - /bin/sparrow-main
                - serve
              env:
                - name: AWS_ACCESS_KEY_ID
                  valueFrom:
                    secretKeyRef:
                      key: AWS_ACCESS_KEY_ID
                      name: my-release-kaskada-canary
                - name: AWS_SECRET_ACCESS_KEY
                  valueFrom:
                    secretKeyRef:
                      key: AWS_SECRET_ACCESS_KEY
                      name: my-release-kaskada-canary
                - name: AWS_REGION
                  value: us-west-2
                - name: AWS_DEFAULT_REGION
                  value: us-west-2
                - name: SPARROW_LOG_FILTER
                  value: egg::=warn,sparrow_=info,info
                - name: SPARROW_LOG_JSON
                  value: "true"
              image: ghcr.io/kaskada-ai/kaskada/engine:v0.5.4
              imagePullPolicy: IfNotPresent
              name: engine
              ports:
                - containerPort: 50052
                  name: grpc
                  protocol: TCP
              resources:
                limits:
                  cpu: 2
                  memory: 8Gi
                requests:
                  cpu: 500m
                  memory: 2Gi
              volumeMounts:
                - mountPath: /tmp
                  name: kaskada-canary-tmp
            - command:
                - /bin/wren
              env:
                - name: DB_DIALECT
                  value: sqlite
                - name: DB_PATH
                  value: /db/kaskada.db
                - name: DB_IN_MEMORY
                  value: "false"
                - name: OBJECT_STORE_BUCKET
                  value: kaskada-bucket
                - name: OBJECT_STORE_PATH
                  value: /data/
                - name: OBJECT_STORE_TYPE
                  value: s3
                - name: AWS_ACCESS_KEY_ID
                  valueFrom:
                    secretKeyRef:
                      key: AWS_ACCESS_KEY_ID
                      name: my-release-kaskada-canary
                - name: AWS_SECRET_ACCESS_KEY
                  valueFrom:
                    secretKeyRef:
                      key: AWS_SECRET_ACCESS_KEY
                      name: my-release-kaskada-canary
                - name: AWS_REGION
                  value: us-west-2
                - name: AWS_DEFAULT_REGION
                  value: us-west-2
                - name: DEBUG
                  value: "false"
                - name: LOG_FORMAT_JSON
                  value: "true"
              image: ghcr.io/kaskada-ai/kaskada/engine:v0.5.4
              imagePullPolicy: IfNotPresent
              name: manager
              ports:
                - containerPort: 50051
                  name: grpc
                  protocol: TCP
                - containerPort: 3365
                  name: rest
                  protocol: TCP
              resources:
                limits:
                  cpu: 250m
                  memory: 250Mi
                requests:
                  cpu: 100m
                  memory: 100Mi
              volumeMounts:
                - mountPath: /db
                  name: kaskada-canary-db
          volumes:
            - name: kaskada-canary-db
              persistentVolumeClaim:
                claimName: kaskada-canary-db
            - ephemeral:
                volumeClaimTemplate:
                  metadata:
                    labels:
                      type: kaskada-canary-tmp
                  spec:
                    accessModes:
                      - ReadWriteOnce
                    resources:
                      requests:
                        storage: 10Gi
                    storageClassName: gp2
              name: kaskada-canary-tmp
  2: |
    apiVersion: v1
    kind: PersistentVolumeClaim
    metadata:
      name: kaskada-canary-db
      namespace: s3-with-key
    spec:
      accessModes:
        - ReadWriteOnce
      resources:
        requests:
          storage: 1Gi
      storageClassName: gp2
      volumeMode: Filesystem
