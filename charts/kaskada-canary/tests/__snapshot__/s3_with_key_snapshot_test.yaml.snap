  4: |
    apiVersion: v1
    kind: Service
    metadata:
      labels:
        app.kubernetes.io/instance: my-release
        app.kubernetes.io/managed-by: Helm
        app.kubernetes.io/name: kaskada-canary
        helm.sh/chart: kaskada-canary-0.0.1
      name: my-release-kaskada-canary
      namespace: s3-with-key
    spec:
      ports:
        - name: grpc
          port: 50051
          protocol: TCP
          targetPort: 50051
        - name: rest
          port: 3365
          protocol: TCP
          targetPort: 3365
      selector:
        app.kubernetes.io/instance: my-release
